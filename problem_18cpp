#include <iostream>
using namespace std;

//problem #18

void ReadRudis(float& R) {
	cout << "Please Enter Number One.\n";
	cin >> R;
}
float CalcCircleArea(float R) {
	float area;
	area = 3.14 * pow(R, 2);
	return area;
}
void PrintArea(float area) {
	cout << "the Area of Circle = " << area << endl;
}

int main() {
	
	//problem #18
	float R;
	ReadRudis(R);
	PrintArea(CalcCircleArea(R));

	

}