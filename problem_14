}
void SwapNumbers(int &num1, int &num2) {
    int temp;
    temp = num1;
    num1 = num2;
    num2 = temp;
}
void Print2Numbers(int num1, int num2) {

    cout << "\n Number1 = " << num1 << " , Number2 = " << num2 << endl;
    
}


